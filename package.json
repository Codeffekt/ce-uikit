{
  "name": "ce-ui-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:docs": "ng serve docs --port 4201",
    "watch": "ng build --watch --configuration development",
    "build:lib": "ng build ce-uikit",
    "publish:lib": "npm run build:lib; npm publish projects/ce-uikit/dist/ce-uikit",
    "build:style": "npx tailwindcss build -i projects/ce-uikit/src/ce-uikit.css -o project/ce-uikit/build/ce-uikit.bundle.css",
    "build:style:prod": "npx tailwindcss build -i projects/ce-uikitsrc/ce-uikit.css -o projects/ce-uikit/build/ce-uikit.bundle.css --minify",
    "build:icons": "npx fantasticon -c projects/ce-uikit/fantasticon.json",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.1.5",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "highlight.js": "^11.8.0",
    "@angular/router": "^16.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.3.2",
    "autoprefixer": "^10.4.16",
    "fantasticon": "^2.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.0.2"
  }
}
